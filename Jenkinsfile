pipeline {
    agent any 
    stages {
        stage('Build') {
            steps {
                echo "Running build in branch ${env.BRANCH_NAME} ...."
                echo "Chaning in wip branch: take 2"
            }
        }
        stage('Test') {
            steps {
                echo "Running tests in branch ${env.BRANCH_NAME} ...."
                echo "Chaning in wip branch: take 2"
            }
        }
    }
}
